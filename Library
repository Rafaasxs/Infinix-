sectionmn5:addButton("ESP Pe√ßas De Arma", function()
local entulhosFolder = workspace:WaitForChild("Utilitarios"):WaitForChild("Entulhos")


local cores = {
	["Basico"] = Color3.fromRGB(144, 238, 144), 
	["Epic"]   = Color3.fromRGB(148, 0, 211),   
	["Medio"]  = Color3.fromRGB(255, 255, 0),   
	["Raro"]   = Color3.fromRGB(0, 0, 255)      
}

local function highlight(objeto)
	
	if objeto:IsA("Model") and objeto.Name == "Rubble Mesh" then
		
		
		local categoria = objeto.Parent.Name
		local corescolhida = cores[categoria]
		
		if corescolhida then
			
			local highlight = objeto:FindFirstChildOfClass("Highlight") or Instance.new("Highlight")
			highlight.Parent = objeto
			highlight.FillColor = corescolhida
			highlight.OutlineColor = Color3.new(1, 1, 1) 
			highlight.FillTransparency = 0.5
			highlight.OutlineTransparency = 0
		end
	end
end


for _, descendente in pairs(entulhosFolder:GetDescendants()) do
	highlight(descendente)
end


entulhosFolder.DescendantAdded:Connect(function(novoObjeto)
	
	task.wait(0.1) 
	highlight(novoObjeto)
end)

end
)

