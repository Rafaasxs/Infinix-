        local encontrou = false
    
        for _, l in ipairs(workspace.MapaGeral.Gari.Lixos:GetChildren()) do
            local pp = l:FindFirstChildOfClass("ProximityPrompt")
            if pp and pp.Enabled then
                encontrou = true
