    tpws = v
end)

rs.Heartbeat:Connect(function(delta)
    if tpws > 0 and chr and hum and hum.Parent and hum.MoveDirection.Magnitude > 0 then
        chr:TranslateBy(hum.MoveDirection * delta * tpws)
    end
end)





