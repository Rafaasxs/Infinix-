sectionmn2:addButton("Anti-Staff", function()
    local p = game:GetService("Players")
local sg = game:GetService("StarterGui")
local lastState = nil
local lastStaffs = ""

local function checkStaffs()
    local t = {}
    for _, v in ipairs(p:GetPlayers()) do
        if v.Team and v.Team.Name == "STAFF" then
            table.insert(t, v.Name)
        end
    end
    local hasStaff = #t > 0
    local staffNames = table.concat(t, ", ")

    if hasStaff ~= lastState or (hasStaff and staffNames ~= lastStaffs) then
        lastState = hasStaff
        lastStaffs = staffNames
        if hasStaff then
            sg:SetCore("SendNotification", {Title = "‚ö†Ô∏è Detectado Staffs!", Text = "Nomes: " .. staffNames, Duration = 5})
        else
            sg:SetCore("SendNotification", {Title = "‚úÖ Nenhum Staff", Text = "N√£o h√° staff no jogo", Duration = 3})
        end
    end
end

task.spawn(function()
    while task.wait(5) do
        checkStaffs()
    end
end)

 end)

 sectionmn2:addButton("Desativar dano de queda", function()
    local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

humanoid.HealthChanged:Connect(function(health)
	if humanoid.FloorMaterial ~= Enum.Material.Air and health < humanoid.MaxHealth then
		humanoid.Health = humanoid.MaxHealth
	end
end)

 end)

