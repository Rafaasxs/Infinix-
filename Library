-- ESP 

local uis = game:GetService("UserInputService")
local plrs = game:GetService("Players")
local cam = game:GetService("Workspace").CurrentCamera
local rs = game:GetService("RunService")
local lp = plrs.LocalPlayer

local espEnabled = false
local showBoxes = false
local showNames = false
local showTracers = false
local showHealth = false 

local function getTeamColor(player)
    if player.Team and player.Team.TeamColor then
        return player.Team.TeamColor.Color
    end
    return Color3.fromRGB(255, 255, 255)
end


local function getHealthColor(humanoid)
    local percent = humanoid.Health / humanoid.MaxHealth
    return Color3.new(1 - percent, percent, 0)
end

local function createDrawing(type, props)
    local obj = Drawing.new(type)
    for i, v in pairs(props) do
        obj[i] = v
    end
    return obj
end

local espObjects = {}

local function updateESP()
    for _, v in pairs(espObjects) do
        for _, obj in pairs(v) do
            obj.Visible = false
        end
    end

    if not espEnabled then return end

    for _, v in ipairs(plrs:GetPlayers()) do
        if v ~= lp and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Humanoid") then
            local hrp = v.Character.HumanoidRootPart
            local hum = v.Character.Humanoid
            local pos, vis = cam:WorldToViewportPoint(hrp.Position)
            
            if vis then
                local color = getTeamColor(v)

                if not espObjects[v] then
                    espObjects[v] = {
                        box = createDrawing("Square", { Thickness = 1, Transparency = 1, Filled = false, ZIndex = 2 }),
                        name = createDrawing("Text", { Size = 18, Transparency = 1, Center = true, Outline = true, ZIndex = 3 }),
                        tracer = createDrawing("Line", { Thickness = 1, Transparency = 1, ZIndex = 1 }),
                       
                        healthOutline = createDrawing("Square", { Thickness = 1, Transparency = 1, Filled = true, Color = Color3.new(0,0,0), ZIndex = 2 }),
                        healthBar = createDrawing("Square", { Thickness = 1, Transparency = 1, Filled = true, ZIndex = 3 })
                    }
                end
                
                local esp = espObjects[v]
                local sizeY = (cam:WorldToViewportPoint(hrp.Position + Vector3.new(0, 3, 0)).Y - pos.Y) * 2
                local sizeX = sizeY * 0.6
                local boxPos = Vector2.new(pos.X - sizeX / 2, pos.Y - sizeY / 2)

                
                if showBoxes then
                    esp.box.Size = Vector2.new(sizeX, sizeY)
                    esp.box.Position = boxPos
                    esp.box.Color = color
                    esp.box.Visible = true
                end

             
                if showHealth then
                    local barHeight = (hum.Health / hum.MaxHealth) * sizeY
                    local barColor = getHealthColor(hum)
                    
                    
                    esp.healthOutline.Size = Vector2.new(4, sizeY)
                    esp.healthOutline.Position = Vector2.new(boxPos.X - 6, boxPos.Y)
                    esp.healthOutline.Visible = true
                    
                    -- Barra colorida funcional
                    esp.healthBar.Size = Vector2.new(2, barHeight)
                    esp.healthBar.Position = Vector2.new(boxPos.X - 5, boxPos.Y + (sizeY - barHeight))
                    esp.healthBar.Color = barColor
                    esp.healthBar.Visible = true
                end

                -- L√≥gica do Nome
                if showNames then
                    esp.name.Position = Vector2.new(pos.X, boxPos.Y - 18)
                    esp.name.Text = v.Name
                    esp.name.Color = color
                    esp.name.Visible = true
                end

                -- L√≥gica dos Tracers
                if showTracers then
                    esp.tracer.From = Vector2.new(cam.ViewportSize.X / 2, cam.ViewportSize.Y)
                    esp.tracer.To = Vector2.new(pos.X, pos.Y + sizeY / 2)
                    esp.tracer.Color = color
                    esp.tracer.Visible = true
                end
            end
        end
    end
end

rs.RenderStepped:Connect(updateESP)

-- Bot√µes da sua UI
sectionpvp2:addToggle("Ativar ESP", false, function(value)
    espEnabled = value
end)

sectionpvp2:addToggle("Boxes", false, function(value)
    showBoxes = value
end)

sectionpvp2:addToggle("Names", false, function(value)
    showNames = value
end)

sectionpvp2:addToggle("Health Bar", false, function(value) -- Novo Bot√£o
    showHealth = value
