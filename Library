local function arrastar_ui()
    local script = Instance.new("LocalScript", Olokow)
    local u = game:GetService("UserInputService")
    local p = game.Players.LocalPlayer
    local m = p:GetMouse()
    local g = script.Parent
    local d = false
    local o = Vector2.new()
    local s = game:GetService("RunService").RenderStepped

    g.InputBegan:Connect(function(i)
        if i.UserInputType == Enum.UserInputType.MouseButton1 then
            d = true
            o = Vector2.new(m.X - g.AbsolutePosition.X, m.Y - g.AbsolutePosition.Y)
        end
    end)

    g.InputEnded:Connect(function(i)
        if i.UserInputType == Enum.UserInputType.MouseButton1 then
            d = false
        end
    end)

    s:Connect(function()
        if d then
            local t = Vector2.new(m.X - o.X, m.Y - o.Y)
            g.Position = g.Position:Lerp(UDim2.new(0, t.X, 0, t.Y), 0.2)
        end
    end)
end
coroutine.wrap(arrastar_ui)()

