-- sections pvp 4

_G.HitboxEnabled = false
_G.HitboxSize = 2
_G.HitboxPart = "Head"
_G.HitboxTransparency = 0.5

local function GetRealPart(character, partName)
    if partName == "Head" then return character:FindFirstChild("Head") end
    if partName == "Torso" then return character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso") or character:FindFirstChild("HumanoidRootPart") end
    if partName == "Right Arm" then return character:FindFirstChild("Right Arm") or character:FindFirstChild("RightUpperArm") end
    if partName == "Left Arm" then return character:FindFirstChild("Left Arm") or character:FindFirstChild("LeftUpperArm") end
    if partName == "Right Leg" then return character:FindFirstChild("Right Leg") or character:FindFirstChild("RightUpperLeg") end
    if partName == "Left Leg" then return character:FindFirstChild("Left Leg") or character:FindFirstChild("LeftUpperLeg") end
    return nil
end

local function ResetHitbox()
    for _, v in pairs(game:GetService("Players"):GetPlayers()) do
        if v ~= game:GetService("Players").LocalPlayer and v.Character then
            for _, part in pairs(v.Character:GetChildren()) do
                if part:IsA("BasePart") then
                    if part.Name == "Head" then
                        part.Size = Vector3.new(2, 1, 1)
                    elseif part.Name:find("Torso") then
                        part.Size = Vector3.new(2, 2, 1)
                    elseif part.Name:find("Arm") or part.Name:find("Leg") then
                        part.Size = Vector3.new(1, 2, 1)
                    end
                    part.Transparency = 0
                    part.CanCollide = true
                end
            end
        end
    end
end

local function ApplyHitbox()
    if not _G.HitboxEnabled then return end
    for _, v in pairs(game:GetService("Players"):GetPlayers()) do
        if v ~= game:GetService("Players").LocalPlayer and v.Character then
            local part = GetRealPart(v.Character, _G.HitboxPart)
            if part then
                part.Size = Vector3.new(_G.HitboxSize, _G.HitboxSize, _G.HitboxSize)
                part.Transparency = _G.HitboxTransparency
                part.CanCollide = false
            end
        end
    end
end

sectionpvp4:addToggle("Ativar Hitbox", false, function(value)
    _G.HitboxEnabled = value
    if not value then
        ResetHitbox()
    end
end)

sectionpvp4:addDropdown("Parte do Corpo", {"Head", "Torso", "Right Arm", "Left Arm", "Right Leg", "Left Leg"}, function(text)
    ResetHitbox()
    _G.HitboxPart = text
end)

sectionpvp4:addSlider("Tamanho da Hitbox", 2, 2, 10, function(value)
    _G.HitboxSize = value
end)

game:GetService("RunService").RenderStepped:Connect(function()
    if _G.HitboxEnabled then
        ApplyHitbox()
    end
end)

